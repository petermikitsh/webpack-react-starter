{
  "name": "webpack-react-starter",
  "version": "0.0.0",
  "description": "A boilerplate for developing realtime web applications.",
  "license": "MIT",
  "engines": {
    "node": "5.5.0"
  },
  "scripts": {
    "clean": "rm -rf .build",
    "build:backend": "webpack -p --config server/webpack/webpack.production.backend.js --progress --profile --colors",
    "build:frontend": "webpack -p --config server/webpack/webpack.production.frontend.js --progress --profile --colors",
    "build": "npm run build:backend && npm run build:frontend",
    "dev": "node server/server.babel.js",
    "prod": "node .build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/petermikitsh/webpack-react-starter.git"
  },
  "author": "Peter Mikitsh",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "babel-register": "^6.4.3",
    "body-parser": "^1.14.2",
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "history": "^2.0.0-rc2",
    "immutable": "^3.7.6",
    "material-ui": "^0.14.4",
    "normalize.css": "^3.0.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.7",
    "react-redux": "^4.1.2",
    "react-router": "^2.0.0-rc5",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.1.7",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^1.0.3",
    "socketcluster": "^4.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.2",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "sass-loader": "^3.1.2",
    "source-map-support": "^0.4.0",
    "stream": "0.0.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "1.12.11",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.0"
  }
}
